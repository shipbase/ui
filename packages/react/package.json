{
  "name": "@ui/react",
  "type": "module",
  "private": true,
  "exports": {
    "./examples": {
      "types": "./src/examples/index.ts",
      "default": "./dist/examples/index.js"
    },
    "./examples/*": {
      "source": "./src/examples/*.tsx",
      "types": "./src/examples/*.tsx",
      "default": "./dist/examples/*.js"
    },
    "./*": {
      "source": "./src/components/ui/*.tsx",
      "types": "./src/components/ui/*.tsx",
      "default": "./dist/components/ui/*.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "vite build --watch",
    "clean": "git clean -xdf node_modules .astro .turbo dist",
    "build": "vite build",
    "typecheck": "tsgo --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@ark-ui/react": "catalog:",
    "@chromatic-com/storybook": "catalog:",
    "@storybook/addon-essentials": "catalog:",
    "@storybook/addon-interactions": "catalog:",
    "@storybook/addon-links": "catalog:",
    "@storybook/addon-onboarding": "catalog:",
    "@storybook/addon-themes": "catalog:",
    "@storybook/blocks": "catalog:",
    "@storybook/react": "catalog:",
    "@storybook/react-vite": "catalog:",
    "@storybook/test": "catalog:",
    "@tailwindcss/vite": "catalog:",
    "@types/react": "catalog:react18",
    "@types/react-dom": "catalog:react18",
    "@ui/tailwind": "workspace:*",
    "@ui/typescript": "workspace:*",
    "@vitejs/plugin-react": "catalog:",
    "autoprefixer": "catalog:",
    "class-variance-authority": "catalog:",
    "clsx": "catalog:",
    "globby": "catalog:",
    "lucide-react": "catalog:",
    "react": "catalog:react18",
    "react-dom": "catalog:react18",
    "recharts": "catalog:",
    "storybook": "catalog:",
    "tailwind-merge": "catalog:",
    "tailwindcss": "catalog:",
    "tailwindcss-animate": "catalog:",
    "tw-animate-css": "catalog:",
    "vite": "catalog:",
    "vite-plugin-inspect": "catalog:",
    "vite-tsconfig-paths": "catalog:"
  }
}
